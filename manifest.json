{
    "name": "AWS Doc Language Switcher",
    "version": "1.0",
    "manifest_version": 3,
    "description": "Switch language to Japanese if the page is in English on a specific domain.",
    "icons": {
      "48": "icon_48.png",
      "128": "icon_128.png"
    },
    "host_permissions": [
        "*://*/*"
      ],
    "permissions": [
      "history"
    ],
    "action": {
      "default_icon": "icon_48.png",
      "default_popup": "popup.html"
    }
  }